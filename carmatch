    { brand: "Rivian", country: "USA", language: "en-US", logo: "https://www.carlogos.org/car-logos/rivian-logo.png", flag: "https://flagcdn.com/w40/us.png", tier: "premium" },
    { brand: "Rolls-Royce", country: "United Kingdom", language: "en-GB", logo: "https://www.carlogos.org/car-logos/rolls-royce-logo.png", flag: "https://flagcdn.com/w40/gb.png", tier: "exotic" },
    { brand: "Simca", country: "France", language: "fr-FR", logo: "https://www.carlogos.org/car-logos/simca-logo.png", flag: "https://flagcdn.com/w40/fr.png", tier: "defunct" },
    { brand: "Smart", country: "Germany", language: "de-DE", logo: "https://www.carlogos.org/car-logos/smart-logo.png", flag: "https://flagcdn.com/w40/de.png", tier: "mainstream" },
    { brand: "Talbot", country: "France", language: "fr-FR", logo: "https://www.carlogos.org/car-logos/talbot-logo.png", flag: "https://flagcdn.com/w40/fr.png", tier: "defunct" },
    { brand: "Togg", country: "Turkey", language: "tr-TR", logo: "https://www.carlogos.org/car-logos/togg-logo.png", flag: "https://flagcdn.com/w40/tr.png", tier: "mainstream" },
    { brand: "VinFast", country: "Vietnam", language: "vi-VN", logo: "https://www.carlogos.org/car-logos/vinfast-logo.png", flag: "https://flagcdn.com/w40/vn.png", tier: "mainstream" },
    { brand: "Wiesmann", country: "Germany", language: "de-DE", logo: "https://www.carlogos.org/car-logos/wiesmann-logo.png", flag: "https://flagcdn.com/w40/de.png", tier: "exotic" },
    { brand: "Xpeng", country: "China", language: "zh-CN", logo: "https://www.carlogos.org/car-logos/xpeng-logo.png", flag: "https://flagcdn.com/w40/cn.png", tier: "premium" },
    { brand: "Zastava", country: "Serbia", language: "sr-RS", logo: "https://www.carlogos.org/car-logos/zastava-logo.png", flag: "https://flagcdn.com/w40/rs.png", tier: "defunct" }
];

        let score = 0;
        let selectedBrand = null;
        let lastCountry = null;
        let anchorPoint = null;
        const incorrectBrands = new Set();
        const brandsDiv = document.getElementById('brands');
        const mapSvg = document.getElementById('world-map');
        const scoreDisplay = document.getElementById('score');
        const correctMatches = document.getElementById('correct-matches');
        const incorrectMatches = document.getElementById('incorrect-matches');
        const ttsLanguageSelect = document.getElementById('tts-language');
        let connectionLine = null;

        function speak(text, language) {
            if (!text || !language) return;
            const utterance = new SpeechSynthesisUtterance(text);
            utterance.lang = language;
            utterance.rate = 1.0;
            speechSynthesis.speak(utterance);
        }

        function triggerConfetti() {
            confetti({
                particleCount: 100,
                spread: 90,
                origin: { y: 0.6 },
                disableForReducedMotion: true
            });
            setTimeout(() => confetti.reset(), 2000);
        }

        function shuffle(array) {
            for (let i = array.length - 1; i > 0; i--) {
                const j = Math.floor(Math.random() * (i + 1));
                [array[i], array[j]] = [array[j], array[i]];
            }
            return array;
        }

        function updateLineToCursor(event) {
            if (!selectedBrand || !anchorPoint || !connectionLine) return;
            const svgRect = mapSvg.getBoundingClientRect();
            let x2 = ((event.clientX - svgRect.left) * 1000 / svgRect.width);
            let y2 = ((event.clientY - svgRect.top) * 500 / svgRect.height);

            // Calculate distance from anchor point to cursor
            const dx = x2 - anchorPoint.x;
            const dy = y2 - anchorPoint.y;
            const distance = Math.sqrt(dx * dx + dy * dy);

            // Cap the maximum length of the line to prevent arrowhead separation
            const maxLength = 1000;
            if (distance > maxLength) {
                const scale = maxLength / distance;
                x2 = anchorPoint.x + dx * scale;
                y2 = anchorPoint.y + dy * scale;
            }

            connectionLine.setAttribute('x1', anchorPoint.x);
            connectionLine.setAttribute('y1', anchorPoint.y);
            connectionLine.setAttribute('x2', x2);
            connectionLine.setAttribute('y2', y2);
            connectionLine.classList.add('draw');
        }

        function clearLine() {
            if (!connectionLine) return;
            connectionLine.classList.remove('draw');
            connectionLine.setAttribute('x1', 0);
            connectionLine.setAttribute('y1', 0);
            connectionLine.setAttribute('x2', 0);
            connectionLine.setAttribute('y2', 0);
            anchorPoint = null;
        }

        function showTab(tier) {
            const tabs = document.querySelectorAll('.tab');
            const lists = document.querySelectorAll('.brand-list');
            tabs.forEach(tab => tab.classList.remove('active'));
            lists.forEach(list => list.style.display = 'none');
            document.querySelector(`.tab[onclick="showTab('${tier}')"]`).classList.add('active');
            document.getElementById(tier).style.display = 'block';
        }

        function updateCountryLabels() {
            const selectedLang = ttsLanguageSelect.value;
            const langKey = {
                'pl-PL': 'polish',
                'en-US': 'english',
                'es-ES': 'spanish',
                'fr-FR': 'french'
            }[selectedLang];
            const countryPaths = mapSvg.querySelectorAll('.country-path');
            countryPaths.forEach(path => {
                let sibling = path.nextElementSibling;
                if (sibling && sibling.classList.contains('country-flag')) {
                    sibling = sibling.nextElementSibling;
                    if (sibling && sibling.classList.contains('country-label')) {
                        sibling.textContent = path.dataset[langKey] || path.dataset.english;
                    }
                }
            });
        }

        function updateInterfaceText() {
            const selectedLang = ttsLanguageSelect.value;
            const t = translations[selectedLang];
            scoreDisplay.textContent = `${t.score}: ${score}`;
            document.getElementById('brands-header').textContent = t.carBrands;
            document.getElementById('tab-mainstream').textContent = t.mainstream;
            document.getElementById('tab-premium').textContent = t.premium;
            document.getElementById('tab-exotic').textContent = t.exotic;
            document.getElementById('tab-defunct').textContent = t.defunct;
            document.getElementById('reset-button').textContent = t.reset;
            document.getElementById('map-header').textContent = t.worldMap;
        }

        function initializeGame() {
            connectionLine = document.getElementById('connection-line');
            correctMatches.innerHTML = '<h2>ðŸ˜„</h2>';
            incorrectMatches.innerHTML = '<h2>ðŸ¥µ</h2>';
            score = 0;
            scoreDisplay.textContent = `${translations[ttsLanguageSelect.value].score}: ${score}`;
            selectedBrand = null;
            lastCountry = null;
            anchorPoint = null;
            incorrectBrands.clear();
            connectionLine.setAttribute('x1', 0);
            connectionLine.setAttribute('y1', 0);
            connectionLine.setAttribute('x2', 0);
            connectionLine.setAttribute('y2', 0);
            connectionLine.classList.remove('draw');

            const mainstreamList = document.getElementById('mainstream');
            const premiumList = document.getElementById('premium');
            const exoticList = document.getElementById('exotic');
            const defunctList = document.getElementById('defunct');
            mainstreamList.innerHTML = '';
            premiumList.innerHTML = '';
            exoticList.innerHTML = '';
            defunctList.innerHTML = '';

            const shuffledBrands = shuffle([...carData]);

            shuffledBrands.forEach(item => {
                const brandDiv = document.createElement('div');
                brandDiv.className = 'brand';
                brandDiv.dataset.brand = item.brand;
                brandDiv.dataset.language = item.language;

                const logoImg = document.createElement('img');
                logoImg.src = item.logo;
                logoImg.alt = item.brand;
                logoImg.title = item.brand;
                logoImg.className = 'logo';
                logoImg.onerror = () => {
                    logoImg.src = 'data:image/svg+xml;base64,';
                };

                brandDiv.appendChild(logoImg);
                brandDiv.appendChild(document.createTextNode(item.brand));
                const feedbackSpan = document.createElement('span');
                feedbackSpan.className = 'feedback';
                brandDiv.appendChild(feedbackSpan);
                brandDiv.addEventListener('click', () => {
                    selectBrand(brandDiv);
                    speak(item.brand, item.language);
                });

                if (item.tier === 'mainstream') {
                    mainstreamList.appendChild(brandDiv);
                } else if (item.tier === 'premium') {
                    premiumList.appendChild(brandDiv);
                } else if (item.tier === 'exotic') {
                    exoticList.appendChild(brandDiv);
                } else if (item.tier === 'defunct') {
                    defunctList.appendChild(brandDiv);
                }
            });

            const countryPaths = mapSvg.querySelectorAll('.country-path');
            countryPaths.forEach(path => {
                path.removeEventListener('click', selectCountry);
                path.addEventListener('click', (e) => {
                    lastCountry = e.target.dataset.country;
                    selectCountry(e.target);
                });
            });

            document.addEventListener('mousemove', updateLineToCursor);
            updateCountryLabels();
            updateInterfaceText();
        }

        function selectBrand(brandElement) {
            if (selectedBrand) {
                selectedBrand.classList.remove('selected');
                const feedback = selectedBrand.querySelector('.feedback');
                if (feedback) feedback.innerHTML = '';
                clearLine();
            }
            selectedBrand = brandElement;
            if (selectedBrand) {
                selectedBrand.classList.add('selected');
                const brandRect = selectedBrand.getBoundingClientRect();
                const svgRect = mapSvg.getBoundingClientRect();
                anchorPoint = {
                    x: ((brandRect.right - svgRect.left) * 1000 / svgRect.width),
                    y: ((brandRect.top + brandRect.height / 2) - svgRect.top) * (500 / svgRect.height)
                };
            }
        }

        function selectCountry(countryElement) {
            if (!selectedBrand || !selectedBrand.dataset || !anchorPoint || !connectionLine) return;

            const brand = selectedBrand.dataset.brand;
            const country = countryElement.dataset.country;

            const countryRect = countryElement.getBoundingClientRect();
            const svgRect = mapSvg.getBoundingClientRect();
            const x2 = ((countryRect.left + countryRect.width / 2) - svgRect.left) * (1000 / svgRect.width);
            const y2 = ((countryRect.top + countryRect.height / 2) - svgRect.top) * (500 / svgRect.height);

            connectionLine.setAttribute('x1', anchorPoint.x);
            connectionLine.setAttribute('y1', anchorPoint.y);
            connectionLine.setAttribute('x2', x2);
            connectionLine.setAttribute('y2', y2);
            connectionLine.classList.add('draw');

            const match = carData.find(item => item.brand === brand && item.country === country);
            const logoImg = selectedBrand.querySelector('img.logo')?.cloneNode();
            const feedbackSpan = selectedBrand.querySelector('.feedback');

            if (match && logoImg) {
                logoImg.title = brand;
                if (incorrectBrands.has(brand)) {
                    incorrectBrands.delete(brand);
                    const incorrectImgs = incorrectMatches.querySelectorAll('img');
                    for (let img of incorrectImgs) {
                        if (img.alt === brand) {
                            img.remove();
                            break;
                        }
                    }
                }
                selectedBrand.classList.add('correct');
                correctMatches.appendChild(logoImg);
                score++;
                scoreDisplay.textContent = `${translations[ttsLanguageSelect.value].score}: ${score}`;
                triggerConfetti();

                const selectedLang = ttsLanguageSelect.value;
                const langKey = {
                    'pl-PL': 'polish',
                    'en-US': 'english',
                    'es-ES': 'spanish',
                    'fr-FR': 'french'
                }[selectedLang];
                const countryName = countryElement.dataset[langKey] || countryElement.dataset.english;
                speak(countryName, selectedLang);

                if (score === carData.length) {
                    correctMatches.innerHTML = '<h2>ðŸ˜„</h2><p>Congratulations! You matched all possible brands correctly!</p>';
                    triggerConfetti();
                }
            } else if (logoImg) {
                logoImg.title = brand;
                selectedBrand.classList.add('incorrect');
                if (!incorrectBrands.has(brand)) {
                    incorrectMatches.appendChild(logoImg);
                    incorrectBrands.add(brand);
                }
                const correctCountry = carData.find(item => item.brand === brand).country;
                const correctFlag = carData.find(item => item.brand === brand).flag;
                if (feedbackSpan) {
                    feedbackSpan.innerHTML = `from ${correctCountry} `;
                    const flagImg = document.createElement('img');
                    flagImg.src = correctFlag;
                    flagImg.alt = correctCountry;
                    flagImg.className = 'flag';
                    feedbackSpan.appendChild(flagImg);
                }
                setTimeout(() => {
                    if (selectedBrand) {
                        selectedBrand.classList.remove('incorrect');
                        if (feedbackSpan) feedbackSpan.innerHTML = '';
                    }
                }, 2000);
            }

            if (selectedBrand) {
                selectedBrand.classList.remove('selected');
            }
            selectedBrand = null;
            clearLine();
        }

        function resetGame() {
            document.removeEventListener('mousemove', updateLineToCursor);
            initializeGame();
        }

        ttsLanguageSelect.addEventListener('change', () => {
            updateCountryLabels();
            updateInterfaceText();
        });

        document.addEventListener('DOMContentLoaded', initializeGame);
    </script>
</body>
</html>
